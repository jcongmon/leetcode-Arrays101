class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        size_t cnt {0};
        size_t maxn {0};
        size_t maxnum {0};
        for (size_t i {0}; i < nums.size(); ++i) {
            if (nums.at(i) == 1) cnt++;
            else cnt = 0;
            maxn = cnt;
            maxnum = max(maxnum, maxn);
        }
        return maxnum;
    }
};
