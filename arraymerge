#include <iostream>
#include <vector>
using namespace std;

void merge(vector<int> &nums1, int m, vector<int> &nums2, int n);
void printArray(const vector<int> nums1);
int main() {
    vector<int> nums1 {1, 2, 3, 0, 0, 0}, nums2 {2, 5, 6};
    int m {3}, n {3};
    merge(nums1, m, nums2, n);
    printArray(nums1);
    return 0;
}
void merge(vector<int> &nums1, int m, vector<int> &nums2, int n) {
    int size {m+n-1}, i {m-1}, j {n-1};
    while (i >= 0 && j >= 0) {
        if (nums1[i] > nums2[j]) {
            nums1[size--] = nums1[i--];
        } else {
            nums1[size--] = nums2[j--];
        }
    }
    while (j >= 0) nums1[size--] = nums2[j--];
}
void printArray(const vector<int> nums1) {
    for (int n:nums1) cout << n << " ";
}
